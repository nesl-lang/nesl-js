{
  "cases": [
    {
      "name": "no unknown params",
      "input": [
        ["path", "content"],
        {
          "path": { "type": "string", "required": true },
          "content": { "type": "string", "required": true }
        }
      ],
      "expected": []
    },
    {
      "name": "one unknown param",
      "input": [
        ["path", "content", "extra"],
        {
          "path": { "type": "string", "required": true },
          "content": { "type": "string", "required": true }
        }
      ],
      "expected": ["extra"]
    },
    {
      "name": "typo in param name",
      "input": [
        ["paht", "content"],
        {
          "path": { "type": "string", "required": true },
          "content": { "type": "string", "required": true }
        }
      ],
      "expected": ["paht"]
    }
  ]
}