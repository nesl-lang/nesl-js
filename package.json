{
  "name": "nesl-js",
  "version": "0.1.9",
  "description": "JavaScript parser for NESL format",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "npm run generate:patterns",
    "build": "tsc",
    "generate:patterns": "node scripts/generate-patterns.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nesl-lang/nesl-js.git"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "marked": "^16.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}