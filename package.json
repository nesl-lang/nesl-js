{
  "name": "nesl",
  "version": "0.1.12-beta.0",
  "description": "JavaScript parser for NESL format",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "npm run generate:patterns",
    "build": "tsc",
    "prepublishOnly": "npm run test && npm run build",
    "generate:patterns": "node scripts/generate-patterns.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "clean": "rm -rf dist"
  },
  "keywords": ["nesl", "parser", "language"],
  "author": "Stuart Robinson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nesl-lang/nesl-js.git"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "marked": "^16.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}