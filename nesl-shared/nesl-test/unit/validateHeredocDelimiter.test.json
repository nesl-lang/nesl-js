{
  "function": "validateHeredocDelimiter",
  "groups": [
    {
      "name": "valid-delimiters",
      "tests": [
        {
          "name": "correct-id",
          "input": ["EOT_NESL_abc", "abc"],
          "expected": { "valid": true }
        }
      ]
    },
    {
      "name": "mismatch-id",
      "tests": [
        {
          "name": "wrong-id",
          "input": ["EOT_NESL_xyz", "abc"],
          "expected": {
            "valid": false,
            "error": "Heredoc delimiter must be 'EOT_NESL_abc'"
          }
        },
        {
          "name": "wrong-prefix",
          "input": ["END_NESL_abc", "abc"],
          "expected": {
            "valid": false,
            "error": "Heredoc delimiter must be 'EOT_NESL_abc'"
          }
        },
        {
          "name": "lowercase-prefix",
          "input": ["eot_nesl_abc", "abc"],
          "expected": {
            "valid": false,
            "error": "Heredoc delimiter must be 'EOT_NESL_abc'"
          }
        },
        {
          "name": "extra-suffix",
          "input": ["EOT_NESL_abc_extra", "abc"],
          "expected": {
            "valid": false,
            "error": "Heredoc delimiter must be 'EOT_NESL_abc'"
          }
        },
        {
          "name": "partial-match",
          "input": ["EOT_NESL_ab", "abc"],
          "expected": {
            "valid": false,
            "error": "Heredoc delimiter must be 'EOT_NESL_abc'"
          }
        }
      ]
    }
  ]
}